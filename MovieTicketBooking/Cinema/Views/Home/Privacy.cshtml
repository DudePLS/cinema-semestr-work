@{
    ViewData["Title"] = "За скидками обращаться сюда";
}
<h1>@ViewData["Title"]</h1>
<p>г.Казань,Кремлевская, 32</p>

<head>
    <title></title>

    <style type="text/css">
        /* Set the size of the div element that contains the map */
        #map {
            height: 400px;
            /* The height is 400 pixels */
            width: 100%;
            /* The width is the width of the web page */
        }
    </style>
    <script>
        // Initialize and add the map
        function initMap() {
            // The location of Uluru
            const uluru = { lat: 55.7922428, lng: 49.1221212 };
            // The map, centered at Uluru
            const map = new google.maps.Map(document.getElementById("map"), {
                zoom: 15,
                center: uluru,
            });
            // The marker, positioned at Uluru
            const marker = new google.maps.Marker({
                position: uluru,
                map: map,
            });
        }
    </script>
</head>
<body>
    <h4>Группа 11-912</h4>
    <!--The div element for the map -->
    <div id="map"></div>

    <!-- Async script executes immediately and must be after any DOM elements used in callback. -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC62fN1YJqRiKYIWDZRYbF8IOIyzPUiVC8&callback=initMap&libraries=&v=weekly"
            async></script>
</body>
